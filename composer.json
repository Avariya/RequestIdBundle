{
    "name": "avariya/request-id-bundle",
    "license": "MIT",
    "description": "Adding request id to project flow",
    "keywords": ["request id", "debug"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Oleksii Romanchenko",
            "email": "avariya1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^3.3|^4.0",
        "symfony/expression-language": "^3.3|^4.0",
        "symfony/yaml": "^3.3|^4.0",
        "qandidate/stack-request-id": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "symfony/phpunit-bridge": "^3.3|^4.0",
        "squizlabs/php_codesniffer": "^3.0",
        "sebastian/phpcpd": "^3.0|^4.0",
        "phpmd/phpmd" : "^2.6",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^4.0",
        "phpstan/phpstan": "^0.9",
        "csa/guzzle-bundle": "^2.0|^3.0",
        "symfony/monolog-bundle": "^2.0|^3.0",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": { "Avariya\\RequestIdBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Avariya\\RequestIdBundle\\": "tests/" }
    },
    "suggest": {
        "csa/guzzle-bundle": "Pass request id to all called services",
        "symfony/monolog-bundle": "Log request id"
    },
    "config": {
        "bin-dir": "bin"
    }
}
